    "@nx-tools/container-metadata": "^6.1.0",
    "@nx-tools/nx-container": "^6.1.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
