{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@quassel/utils",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "ES2024",
      "DOM"
    ],
    "module": "ESNext",
    "target": "ES6",
    "resolveJsonModule": true,
    "noEmit": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
  }
}